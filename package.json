    "lint": "tslint --project ./tsconfig.json",
    "lint:autofix": "npm run lint -- --fix",
    "start": "cross-env NODE_ENV=development webpack -d",
    "test": "jest",
    "test:format": "prettier --config=./.prettierrc --check '{**/,}*.{json,md,ts,tsx,scss}' --ignore-path .gitignore",
    "typecheck": "tsc --noEmit"
  },
  "author": "Fausto Núñez Alberro <fausto.nunez@mailbox.org> (https://brainlessdeveloper.com)",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/copy-webpack-plugin": "^5.0.0",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/node": "^12.0.10",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "@types/uuid": "^3.4.4",
    "@types/webpack": "^4.4.34",
    "autoprefixer": "^9.6.0",
    "babel-loader": "^8.0.6",
