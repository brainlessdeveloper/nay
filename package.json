    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "file-loader": "^4.0.0",
    "fork-ts-checker-webpack-plugin": "^1.3.7",
    "html-webpack-plugin": "^3.2.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.18.2",
    "sass": "^1.22.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.2",
    "web-ext-types": "^3.1.0",
    "webextension-polyfill": "^0.4.0",
